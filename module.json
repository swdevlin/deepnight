{
  "id": "deepnight",
  "title": "Deepnight Revelation Campaign Tracker",
  "description": "A module to keep track the various numbers used in the Deepnight Revelation campaign by Mongoose Publishing.",
  "version": "0.1.3",
  "manifestPlusVersion": "0.1.3",
  "authors": [
    {
      "id": "RadioFreeWABA",
      "email": "swdevlin@hey.com",
      "name": "Shawn Devlin",
      "url": "https://openmuse.info"
    }
  ],
  "compatibility": {
    "minimum": "10",
    "verified": "10"
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "esmodules": [
    "deepnight.js"
  ],
  "styles": [
    "./styles/deepnight.css"
  ],
  "packs": [
    {
      "name": "ei-resolution",
      "label": "D/CEI Resolution",
      "path": "./packs/rolltables/cei-resolution.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "ceim-changes",
      "label": "CEIM Changes",
      "path": "./packs/rolltables/ceim-changes.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "mishaps",
      "label": "Mishaps",
      "path": "./packs/rolltables/mishaps.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "incidents",
      "label": "Incidents",
      "path": "./packs/rolltables/incidents.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "opportunities",
      "label": "Opportunities",
      "path": "./packs/rolltables/opportunities.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "crisis-nature",
      "label": "Crisis: Nature",
      "path": "./packs/rolltables/crisis-nature.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    },
    {
      "name": "crisis-severity",
      "label": "Crisis: Severity",
      "path": "./packs/rolltables/crisis-severity.db",
      "module": "deepnight",
      "type": "RollTable",
      "entity": "RollTable"
    }
  ]
}
